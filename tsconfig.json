{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext",
        "es6"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "strictNullChecks": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "outDir": "dist"
    },
    "include": [
      "*.ts","util/*.ts","questions/*.ts"
    ],
    "exclude": ["node_modules"]
  }